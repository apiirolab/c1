{
  "path-intellisense.mappings": {
    "/": "${workspaceRoot}",
    "data": "${workspaceRoot}/data",
    "client": "${workspaceRoot}/client"
  },
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    "**/tests/**",
    ".vscode",
    "typings",
    "prisma.ts"
  ],
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "go",
    "handlebars",
    "html",
    "jade",
    "javascript",
    "javascriptreact",
    "json",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pub",
    "python",
    "restructuredtext",
    "rust",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yml"
  ],
  "path-autocomplete.pathMappings": {
    "data": "${folder}/src/data",
    "client": "${folder}/src/client",
    "server": "${folder}/src/server"
  }
}
